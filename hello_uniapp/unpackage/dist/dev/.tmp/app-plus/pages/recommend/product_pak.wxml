<view class="product_pak_page"><toprow vue-id="1" titleval="产品礼包" bind:__l="__l"></toprow><view class="top_free"><view class="product_list"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="product_item"><image class="product_image" src="{{item.original_img}}" data-event-opts="{{[['tap',[['goDetail',['$0'],[[['dataList','',index,'goods_id']]]]]]]}}" bindtap="__e"></image><view class="product_name">{{item.goods_name}}</view><view class="product_price">{{'￥'+item.shop_price+''}}<block wx:if="{{isVip==0}}"><button class="product_get" hover-class="hover-class" data-event-opts="{{[['tap',[['backBeforePage',['$event']]]]]}}" bindtap="__e">升级领</button></block><block wx:else><button class="product_get" hover-class="hover-class" data-event-opts="{{[['tap',[['getThisProduct',['$0'],[[['dataList','',index]]]]]]]}}" bindtap="__e">点击领</button></block></view></view></block><block wx:if="{{listIsFree}}"><no-data vue-id="2" bind:__l="__l"></no-data></block></view></view><block wx:if="{{isVip==0}}"><button class="member_btn purple_btn" hover-class="none" data-event-opts="{{[['tap',[['backBeforePage',['$event']]]]]}}" bindtap="__e">立即升级掌柜</button></block></view>