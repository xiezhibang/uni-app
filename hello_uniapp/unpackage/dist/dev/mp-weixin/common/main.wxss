










































/*每个页面公共css */
page{
	background: #fff;
}
.navigator-hover{
	background: none;
	opacity: 1;
}
.purple_btn{
	width: 670rpx;
	height: 80rpx;
	border-radius: 40rpx;
	background: #8A3EDF;
	color: #fff;
	text-align: center;
	font-size: 32rpx;
	line-height: 80rpx;
	border:none;
	outline: none;
	margin: auto;
}
.head_out{
	position: fixed;
	z-index: 9999;
	top:0;
	left: 0;
	width: 750rpx;
	height: auto;
	background: #fff;
}
.title_padding{
	width: 100%;
	height: 25px;
	
	height: calc(44rpx + 25px);
	
}
.title_padding.fix{
	position:fixed;
	top:0;
	left: 0;
	z-index: 999;
	width: 100%;
	height: 25px;
	
	height: calc(44rpx + 25px);
	
}
.top_free{
	width: 750rpx;
	padding-top: calc(128rpx + 25px);
	
	padding-top: calc(172rpx + 25px);
	
}

/*通用样式区*/
.entityObj{
	width: 100%;
	height: 100%;
	display: block;
}
.baseObj{
	width: 100%;
	display: block;
}
.floatObj{
	position: absolute;
	z-index: 2;
}
.floatObjInit{
	position: absolute;
	top:0;
	left: 0;
	z-index: 2;
}
.centerObj{
	position: absolute;
	top:50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
	z-index: 2;
}


