.swiper_box{
	width: 750rpx;
	height: 450rpx;
	position: relative;
}
.swiper_box swiper{
	height: 450rpx;
}
.swiper-item > image{
	width:100%;
	height: 450rpx;
}
.sw_num{
	padding:0 24rpx;
	width: auto;
	height: 44rpx;
	line-height: 44rpx;
	background: rgba(0,0,0,0.3);
	position: absolute;
	right: 40rpx;
	bottom: 30rpx;
	font-size: 30rpx;
	color: #fff;
	border-radius: 22rpx;
	z-index: 100;
}
.sw_btn{
	position: absolute;
	top:70rpx;
	width: 52rpx;
	height: 52rpx;
	z-index: 100;
	border-radius: 100%;
	overflow: hidden;
}
.shareBtn{
	position: absolute;
	top:0;
	left: 0;
	width: 100%;
	height: 100%;
	border-radius: 100%;
	z-index: 20;
	opacity:0;
}
.sw_btn > image{
	width: 100%;
	height: 0;
	display: block;
}
.sw_btn.back{
	left: 30rpx;
	z-index: 200;
}
.sw_btn.love{
	right: 110rpx;
}
.sw_btn.share{
	right: 30rpx;
}
.share_block{
	position: absolute;
	top:86rpx;
	right: 0;
	width: 200rpx;
	height: auto;
	background: #fff;
	border-radius: 6rpx;
}
.share_block::before{
	content: "";
	width: 30rpx;
	height: 24rpx;
	position: absolute;
	top:-11rpx;
	right: 10rpx;
	background: #fff;
	-webkit-transform: rotate(-34deg) skewY(-15deg);
	        transform: rotate(-34deg) skewY(-15deg);
}
.share_line{
	width: 100%;
	height: 80rpx;
	position: relative;
	box-sizing: border-box;
	padding-left: 70rpx;
	font-size: 26rpx;
	line-height: 80rpx;
}
.share_line > image{
	width: 30rpx;
	height: 0;
	position: absolute;
	top:50%;
	left:20rpx;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.shop_name{
	position: relative;
	width: 750rpx;
	padding:20rpx 150rpx 20rpx 30rpx;
	box-sizing: border-box;
	font-size: 36rpx;
	color: #000000;
}
.ct_obj{
	position: absolute;
	top:50%;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.shop_name > .tag{
	right: 30rpx;
	font-size: 28rpx;
	color:#7A37C5;
}
.shop_address{
	width: 750rpx;
	padding:6rpx 150rpx 6rpx 66rpx;
	font-size: 26rpx;
	color: #999999;
	box-sizing: border-box;
	position: relative;
}
.shop_address > image{
	left: 30rpx;
	width: 24rpx;
	height: 0;
	display: block;
}
.shop_address > .phone_call{
	right: 30rpx;
	width: auto;
	height: auto;
	padding:10rpx 20rpx;
}
.phone_call > image{
	width: 32rpx;
	height: 0;
	display: block;
	margin: auto;
}
.custom_line{
	position: relative;
	width: 750rpx;
	padding:10rpx 30rpx;
	box-sizing: border-box;
	font-size: 30rpx;
	color: #000000;
}
.custom_line > text{
	color:#999999;
}
.star_info{
	width: auto;
	height: auto;
	display: inline-block;
}
.avg_price{
	position: absolute;
	right: 30rpx;
	font-size: 34rpx;
	color: #FF0000;
}
.line_grey{
	width: 750rpx;
	height: 10rpx;
	background: #f2f2f2;
	margin: 10rpx 0;
}
.sp_box_item{
	width: 750rpx;
	height: auto;
	background: #fff;
	box-sizing: border-box;
	padding:10rpx 30rpx;
}
.sp_title_line{
	width: 100%;
	height: 50rpx;
	line-height: 50rpx;
	font-size:34rpx;
	color: #000;
	position: relative;
	box-sizing: border-box;
	padding-left: 60rpx;
}
.sp_title_line > image{
	width: 40rpx;
	height: 0;
	display: block;
	position: absolute;
	top:50%;
	left: 0;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.ticket_item{
	width: 100%;
	height: auto;
	margin:10rpx 0;
	padding:16rpx 16rpx;
	box-sizing: border-box;
	background: #F2F2F2;
	border-radius: 12rpx;
	position: relative;
}
.ti_red{
	width: 240rpx;
	height: auto;
	text-align: center;
	color: #fff;
	background: #7A37C5;
	padding:6rpx 0;
	border-radius: 10rpx;
	font-size: 24rpx;
}
.ti_desc,.ti_time{
	width: 82%;
	height: auto;
	font-size: 30rpx;
	margin-top: 12rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-flow: row nowrap;
	        flex-flow: row nowrap;
	text-align: justify;
}
.ti_desc > .left,.ti_time > .left{
	white-space: nowrap;
}
.get_ticker_btn{
	right:16rpx;
	width: 110rpx;
	padding: 0;
	height: 56rpx;
	line-height: 56rpx;
	font-size: 26rpx;
	color: #fff;
	background: #7A37C5;
	border-radius: 12rpx;
	outline: none;
}
.hot_line,.hot_scroll_box{
	width: 100%;
	height: auto;
}
.hot_list{
	width: auto;
	height: 100%;
	display: inline-block;
	white-space: nowrap;
	box-sizing: border-box;
	padding:10rpx 0;
}
.hot_dish{
	width:320rpx;
	display: inline-block;
	margin-right: 50rpx;
}
.hot_dish:last-child{
	margin-right:0;
}
.img_coat{
	width: 320rpx;
	height: 200rpx;
	overflow: hidden;
}
.img_coat > image{
	width: 320rpx;
	height: 0;
}
.hot_letter{
	width: 100%;
	height: auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-flow: row nowrap;
	        flex-flow: row nowrap;
}
.hot_dish_name{
	font-size: 24rpx;
	color: #000000;
	width: 55%;
	white-space: normal;
	word-wrap:break-word;
	word-break:break-all;
}
.hot_dish_price{
	width: 45%;
	font-size: 24rpx;
	color: #FF0000;
	text-align: right;
	white-space: normal;
	word-wrap:break-word;
	word-break:break-all;
}
.introduce_word{
	width: 100%;
	height: auto;
	box-sizing: border-box;
	padding:8rpx 0;
	text-align: justify;
	font-size: 28rpx;
	color:#333;
}
.introduce_word.begin{
	text-indent: 2em;
}
.introduce_img{
	width: 100%;
	height: 0;
	display: block;
	margin:10rpx 0;
}
.over_block{
	width: 100%;
	height: 100rpx;
}
.getTips{
	width: auto;
	height: auto;
	text-align: center;
	background: rgba(0,0,0,0.7);
	position: fixed;
	top:60%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
	padding:20rpx 20rpx;
	border-radius: 12rpx;
}
.getTips > view{
	color: #fff;
	font-size: 34rpx;
}
.getTips > image{
	width: 50rpx;
	height: 0;
	display: inline-block;
	margin-bottom: 6rpx;
}
.lookMore{
	width: 200rpx;
	height: auto;
	padding:8rpx 0;
	margin:auto;
	font-size: 32rpx;
	color: #7A37C5;
	text-align: center;
	text-decoration: underline;
}


