.title_padding{
	background: #262324;
}
.top_block{
	width: 750rpx;
	height: 440rpx;
	background: #262324;
	position: relative;
}
.back_box{
	width: 60rpx;
	height: 50rpx;
	position: absolute;
	top:50rpx;
	left: 24rpx;
	z-index: 200;
}
.back_box > image{
	width: 20rpx;
	height: 0;
}
.mb_title{
	width: 100%;
	left: 0;
	top:70rpx;
	text-align: center;
	color:#fff;
	font-size: 40rpx;
}
.super_mb{
	width: 280rpx;
	position: absolute;
	top:160rpx;
	left: 30rpx;
	height: 60rpx;
	font-size: 36rpx;
	line-height: 60rpx;
	box-sizing: border-box;
	padding-left: 66rpx;
	color: #edc580;
}
.super_mb > image{
	position: absolute;
	top:44%;
	left: 0;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	width: 56rpx;
	height: 0;
}
.buy_two{
	width: auto;
	position: absolute;
	top:160rpx;
	right: 30rpx;
	height: 60rpx;
	font-size: 28rpx;
	line-height: 60rpx;
	text-align: center;
	color: #edc580;
}
.mb_name{
	position: absolute;
	top:250rpx;
	left: 0;
	color: #EDC580;
	width: 100%;
	text-align: center;
	font-size: 60rpx;
	letter-spacing: 2rpx;
}
.mb_name > text{
	font-size: 36rpx;
}
.mb_name > view{
	width: 100%;
	height: auto;
	font-size: 32rpx;
	letter-spacing: 0;
}
.main_list{
	width: 750rpx;
	height: auto;
	background: #f2f2f2;
	padding:15rpx 0;
	box-sizing: border-box;
}
.main_block{
	width: 750rpx;
	height: auto;
	background: #fff;
	padding-bottom: 40rpx;
}
.ml_inner{
	width: 720rpx;
	margin: auto;
	position: relative;
}
.ml_1{
	height: 80rpx;
	line-height: 80rpx;
	font-size: 36rpx;
	color:#333333;
	box-sizing: border-box;
	padding-left: 10rpx;
}
.ml_1_look{
	position: absolute;
	top:50%;
	right: 0;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	width: 166rpx;
	height: 50rpx;
	line-height: 50rpx;
	font-size: 28rpx;
	color:#999999;
}
.ml_1_look > image{
	width: 14rpx;
	height: 0;
	position: absolute;
	top:50%;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	right: 30rpx;
}
.ml_2{
	height: auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-flow: row wrap;
	        flex-flow: row wrap;
	text-align: center;
}
.ml_equity{
	width: 180rpx;
	height: auto;
	margin: 10rpx 0 20rpx 0;
}
.ml_equity > image{
	width: 70rpx;
	height: 70rpx;
}
.ml_word1{
	width: 100%;
	height: auto;
	font-size: 28rpx;
	color: #000000;
	padding-top: 10rpx;
}
.ml_word2{
	font-size: 24rpx;
	width: 100%;
	color: #666666;
	text-align: center;
}
.open_mb_btn{
	width: 680rpx;
	height: 60rpx;
	line-height: 60rpx;
	border: none;
	outline: none;
	margin: 20rpx auto 0 auto;
	background: -webkit-linear-gradient(left,#e6c47d, #dbac5a);
	background: linear-gradient(to right,#e6c47d, #dbac5a);
	color: #fff;
	border-radius: 30rpx;
	font-size: 28rpx;
}
.invite_code{
	width: 690rpx;
	margin: auto;
	height: 120rpx;
	font-size: 32rpx;
	margin-top: 16rpx;
	position: relative;
}
.scanCodeImg{
	width: 50rpx;
	height: 0;
	position: absolute;
	right: 20rpx;
	bottom: 10rpx;
}
.invite_code > input{
	width:86%;
	margin:18rpx 0;
	height: 52rpx;
	border:1rpx solid #999;
	box-sizing: border-box;
	border-radius: 4rpx;
	font-size: 26rpx;
	padding:0 12rpx;
	color: #000000;
}
.pay_btn{
	margin:30rpx auto;
	-webkit-animation: fadeIn 0.6s ease 0.6s 1 alternate;
	        animation: fadeIn 0.6s ease 0.6s 1 alternate;
	-webkit-animation-fill-mode: backwards;
	        animation-fill-mode: backwards;
}
@-webkit-keyframes fadeIn {
from {
    opacity: 0;
}
to {
    opacity: 1;
}
}
@keyframes fadeIn {
from {
    opacity: 0;
}
to {
    opacity: 1;
}
}

