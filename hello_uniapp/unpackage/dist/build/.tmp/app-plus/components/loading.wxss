
.loading_box_max{
	width: 100%;
	height: 100%;
	position: absolute;
	top:0;
	left: 0;
	background: #fff;
	z-index: 2000;
}
.loading_box_max > image{
	position: absolute;
	top:50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
	width: 80rpx;
	height: 80rpx;
}


