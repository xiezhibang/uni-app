.ticket_block{
	width: 700rpx;
	height: 200rpx;
	margin:25rpx auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-flow: row nowrap;
	        flex-flow: row nowrap;
	box-shadow: 2rpx 2rpx 4rpx #EEEEEE,-2rpx -2rpx 4rpx #EEEEEE;
}
.imgs_item{
	width: 200rpx;
	height: 100%;
	position: relative;
	box-sizing: border-box;
	border-right: 1rpx solid #F2F2F2;
}
.tk_type_img{
	width: 130rpx;
	height: 0;
	position: absolute;
	top:0;
	left: 0;
}
.tk_use_img{
	width: 110rpx;
	position: absolute;
	top:28%;
	left: 27%;
	height: 0;
	z-index: 10;
}
.words_item{
	width: 500rpx;
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-flow: column wrap;
	        flex-flow: column wrap;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	font-size: 24rpx;
}
.tk_name{
	width: 100%;
	padding-left: 40rpx;
	box-sizing: border-box;
	font-size: 30rpx;
	color: #333333;
	margin-bottom: 16rpx;
}
.tk_time{
	width: 100%;
	padding-left: 40rpx;
	box-sizing: border-box;
	font-size: 24rpx;
	color: #999999;
	margin-bottom: 5rpx;
}


