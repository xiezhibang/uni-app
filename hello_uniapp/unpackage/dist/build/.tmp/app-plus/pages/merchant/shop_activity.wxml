<view><toprow2 vue-id="1" titleval="店铺活动" bind:__l="__l"></toprow2><view class="fix_box"><view class="white_block"><view class="at_title">活动统计</view><view data-event-opts="{{[['tap',[['goDetail',['$event']]]]]}}" class="at_link" bindtap="__e">详情</view><view data-event-opts="{{[['tap',[['addActivity',['$event']]]]]}}" class="add_at" bindtap="__e">发布活动</view><view class="at_data_show"><view class="data_item"><view class="data_num">{{dataInfo.count}}</view><view class="data_title">领取次数</view><view class="rt_line"></view></view><view class="data_item"><view class="data_num">{{dataInfo.counts}}</view><view class="data_title">使用次数</view><view class="rt_line"></view></view><view class="data_item"><view class="data_num">{{"￥"+dataInfo.couponsum}}</view><view class="data_title">销售金额</view><view class="rt_line"></view></view><view class="data_item"><view class="data_num">{{"￥"+dataInfo.money}}</view><view class="data_title">结算金额</view></view></view></view><view class="at_row"><image src="{{$root.m0}}" mode="widthFix"></image>活动列表</view></view><view class="top_free"><view class="list_block"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="tk_item"><view class="tk_lt"><block wx:if="{{item.type==0}}"><image class="status_img" src="{{$root.m1}}" mode="widthFix"></image></block><block wx:if="{{item.type==1}}"><image class="status_img" src="{{$root.m2}}" mode="widthFix"></image></block><block wx:if="{{item.type==2}}"><image class="status_img" src="{{$root.m3}}" mode="widthFix"></image></block><view class="tk_rate">{{item.rate+"%"}}</view><view class="tk_letter">分润比例</view><block wx:if="{{item.status==0}}"><view data-event-opts="{{[['tap',[['changeStatus',[0,'$0'],[[['dataList','',index,'id']]]]]]]}}" class="tk_btn" bindtap="__e">发放中</view></block><block wx:else><block wx:if="{{item.status==1}}"><view data-event-opts="{{[['tap',[['changeStatus',[1,'$0'],[[['dataList','',index,'id']]]]]]]}}" class="tk_btn grey" bindtap="__e">已取消</view></block><block wx:else><view class="tk_btn grey">已过期</view></block></block></view><view class="tk_rt"><view class="tk_row one"><view>{{item.createnum}}</view><view>{{item.limitmoney}}</view><view>{{item.limitnum}}</view></view><view class="tk_row two"><view>发放数量</view><view>使用限额</view><view>限领次数</view></view><view class="tk_row three">{{"使用期限："+item.use_start_time+"至"+item.use_end_time}}</view><view class="tk_row four">{{"领取期限："+item.send_start_time+"至"+item.send_end_time}}</view></view></view></block></block><block wx:if="{{listIsFree}}"><no-data vue-id="2" tip="暂无活动" bind:__l="__l"></no-data></block></view></view></view>