<view><toprow vue-id="1" titleval="活动券" bind:__l="__l"></toprow><view class="top_free"><view class="top_wrap"><view class="list_control"><view data-event-opts="{{[['tap',[['changeList',[0]]]]]}}" class="{{['list_item_btn',(todoIndex==0)?'active':'']}}" bindtap="__e">未使用</view><view data-event-opts="{{[['tap',[['changeList',[1]]]]]}}" class="{{['list_item_btn',(todoIndex==1)?'active':'']}}" bindtap="__e">已使用</view><view data-event-opts="{{[['tap',[['changeList',[2]]]]]}}" class="{{['list_item_btn',(todoIndex==2)?'active':'']}}" bindtap="__e">已过期</view></view></view><view class="list_box"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="ticket_item"><view class="ti_red">{{"分润比例："+item.rate+"%"}}</view><view class="ti_name">{{''+item.storename+''}}</view><view class="ti_location">{{''+item.provinces+item.citys+item.districts+''}}</view><view class="ti_desc"><view class="left">活动详情：</view><view>{{item.activity_detail}}</view></view><view class="ti_time"><view class="left">活动时间：</view><view>{{item.use_start_time+"至"+item.use_end_time}}</view></view><block wx:if="{{item.type==0}}"><button class="get_ticker_btn ct_obj" hover-class="none">红利</button></block><block wx:if="{{item.type==1}}"><button class="get_ticker_btn ct_obj" hover-class="none">闲置</button></block><block wx:if="{{item.type==2}}"><button class="get_ticker_btn ct_obj" hover-class="none">满减</button></block></view></block><block wx:if="{{listIsFree}}"><no-data vue-id="2" bind:__l="__l"></no-data></block></view></view></view>